                                                            <input asp-for="AssetName" autocomplete="off" class="form-control" onchange="depreciationpercentage()" required />
                                                            <span asp-validation-for="AssetName" class="text-danger"></span>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-sm-4">
                                                    <div class="form-group row">
